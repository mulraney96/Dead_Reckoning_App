<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CompareEquations">

    <TextView
        android:id="@+id/Equation1"
        android:layout_width="223dp"
        android:layout_height="97dp"
        android:background="#BF9C27B0"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/Equation2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.398" />

    <TextView
        android:id="@+id/Equation2"
        android:layout_width="212dp"
        android:layout_height="106dp"
        android:background="#CD009688"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/Equation3"
        android:layout_width="215dp"
        android:layout_height="96dp"
        android:layout_marginBottom="50dp"
        android:background="#C84CAF50"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Equation2" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="2dp"
        android:text="s=V*dT"
        app:layout_constraintBottom_toTopOf="@+id/Equation1"
        app:layout_constraintStart_toStartOf="@+id/Equation1" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="s=(Vf-Vi)/(2*dT)"
        app:layout_constraintBottom_toTopOf="@+id/Equation2"
        app:layout_constraintStart_toStartOf="@+id/Equation2" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="X = Xo + Vo*dT + 1/2 *a* t^2"
        app:layout_constraintBottom_toTopOf="@+id/Equation3"
        app:layout_constraintStart_toStartOf="@+id/Equation3" />
</androidx.constraintlayout.widget.ConstraintLayout>